# This is Git's per-user configuration file.
[user]
	name = Paul Paczuski
	useConfigOnly = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	s = status
	b = branch
	r = reflog
[credential]
	helper = osxkeychain
